# DMF Gateway environment variables

PORT=5000
NODE_ENV=development

# Backend URLs
FIREBASE_BACKEND_URL=https://your-firebase-backend-url
LOVABLE_BACKEND_URL=https://your-lovable-backend-url
DOTNET_BRAIN_URL=http://localhost:5183
PAYMENTS_BACKEND_URL=http://localhost:5200

# Security: API Key (optional for local dev, required for production)
# Leave empty for local development
# In production, set to a strong random key (e.g., generated with: openssl rand -hex 32)
DMF_API_KEY=

# Optional: Logging
LOG_LEVEL=info
